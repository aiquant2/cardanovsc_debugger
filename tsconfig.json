{
	"compilerOptions": {
		"module": "Node16",
		"target": "ES2022",
		"outDir": "out",
        "resolveJsonModule": true,
        "esModuleInterop": true,
		"moduleResolution": "node16",
        "skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"noImplicitAny": true,
		"isolatedModules": true,

		"lib": [
			"ES2022"
		],
		"sourceMap": true,
		"rootDir": "src",
		"strict": true, 
	
	}
}
